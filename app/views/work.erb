<div class="container">
  <ul class="nav nav-tabs nav-fill" id="myTab" role="tablist">
    <li class="nav-item" role="presentation">
      <a class="nav-link active" id="projects-tab" data-toggle="tab" href="#projects" role="tab" aria-controls="projects" aria-selected="true">Code Projects</a>
    </li>
    <li class="nav-item" role="presentation">
      <a class="nav-link" id="graphics-tab" data-toggle="tab" href="#graphics" role="tab" aria-controls="graphics" aria-selected="false">Graphic Design</a>
    </li>
  </ul>
  <div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active" id="projects" role="tabpanel" aria-labelledby="projects-tab">
      <% @projects.each do |project| %>
        <div class="container">
        <h4><%= project.title %></h4>
        <%= project.description %>
        <img class="img-thumbnail" src="<%= project.image %>" alt="">
        <a href="<%= project.github_url %>">GitHub Repo</a>
        </div>
      <% end %>
    </div>
    <div class="tab-pane fade" id="graphics" role="tabpanel" aria-labelledby="graphics-tab">
      <% @graphics.each do |graphic| %>
        <%= graphic.date %>
        <%= graphic.description %>
        <%= graphic.work_hours %>
        <img class="img-fluid" src="<%= graphic.image %>" alt="">
      <% end %>
    </div>
    <div class="tab-pane fade" id="contact" role="tabpanel" aria-labelledby="contact-tab">...</div>
  </div>
</div>
